{
  "author": {
    "Simon Jefford": "si@ibrokethat.com"
  },
  "name": "superposition",
  "description": "Executing code in more than one env at a time.",
  "version": "0.1.0",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "git@github.com:ibrokethat/superposition.git"
  },
  "dependencies": {
    "co": "3.1.x",
    "co-defer": "0.1.x",
    "co-fs": "1.2.x",
    "co-parallel": "1.0.x",
    "co-request": "0.2.x",
    "co-sleep": "0.0.x",
    "config": "0.4.x",
    "jsdom": "git+ssh://git@github.com:ibrokethat/jsdom.git",
    "koa": "0.12.x",
    "koa-router": "3.4.x",
    "moment-timezone": "0.2.x",
    "object-assign": "^1.0.0",
    "pm2": "0.10.x",
    "require-all": "0.0.x",
    "su-define-object": "git+ssh://git@github.com:super-useful/su-define-object.git",
    "su-service": "git+ssh://git@github.com:super-useful/su-service.git",
    "super-func": "0.0.x",
    "super-is": "0.0.x",
    "super-iter": "git+ssh://git@github.com:ibrokethat/iter.git#v0.1.0",
    "super-registry": "0.0.x",
    "useful-copy": "x",
    "useful-value": "x",
    "yaml": "*"
  },
  "devDependencies": {
    "assert": "*",
    "blanket": "1.1.x",
    "chai": "*",
    "esperanto": "^0.3.11",
    "gulp": "^3.8.8",
    "gulp-changed": "^1.0.0",
    "gulp-esnext": "^1.2.0",
    "gulp-esperanto": "^0.0.1",
    "gulp-sourcemaps": "^1.2.2",
    "mocha": "*",
    "rewire": "2.0.x",
    "should": "*",
    "sinon": "*",
    "sinon-chai": "*"
  },
  "scripts": {
    "test": "export NODE_ENV='test' && node --harmony --harmony_typeof ./node_modules/.bin/mocha --colors --harmony --harmony_typeof --recursive -R spec --ui bdd ./test",
    "test-cov": "export NODE_ENV='test' && node --harmony --harmony_typeof ./node_modules/.bin/mocha --colors --harmony --harmony_typeof --recursive --require blanket -R html-cov --ui bdd ./test/**/*.js > test-coverage.html",
    "test-jenkins": "export NODE_ENV='test' && node --harmony --harmony_typeof ./node_modules/.bin/mocha --colors --harmony --harmony_typeof --recursive -R xunit --ui bdd ./test/**/*.js > test-reports.xml",
    "start": "node --harmony --harmony_typeof index.js"
  },
  "config": {
    "blanket": {
      "pattern": [
        "/apis",
        "/lib"
      ]
    }
  }
}
